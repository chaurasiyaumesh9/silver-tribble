{
  "name": "silver-tribble",
  "version": "3.2.0",
  "description": "Webpack based boilerplate with SASS and babel ES6/7 support.",
  "homepage": "https://github.com/chaurasiyaumesh9/silver-tribble.git#readme",
  "scripts": {
    "build-dev": "webpack --mode=development",
    "build-prod": "webpack --mode=production",
    "heroku-postbuild": "webpack -p",
    "start-express": "node server.js",
    "start": "webpack -p --progress && node server.js",
    "dev": "webpack-dev-server",
    "watch": "webpack-dev-server --mode=development --watch",
    "bundle": "npm install && npm run watch",
    "lint-sass": "sass-lint -v -q --format=compact",
    "lint-js": "eslint --ext .js src/js/"
  },
  "keywords": [
    "webpack",
    "boilerplate",
    "template",
    "setup"
  ],
  "author": "UMESH CHAURASIYA",
  "contributors": [
    {
      "name": "Umesh C",
      "email": "chaurasiyaumesh9@gmail.com"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chaurasiyaumesh9/silver-tribble.git"
  },
  "engines": {
    "node": "10.2.1",
    "npm": "5.6.0"
  },
  "dependencies": {
    "autoprefixer": "^9.6.1",
    "bootstrap": "4.3.1",
    "jquery": "3.4.1",
    "popper.js": "1.16.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "@babel/core": "7.7.2",
    "@babel/plugin-proposal-class-properties": "7.7.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "7.7.1",
    "@babel/preset-react": "7.7.0",
    "@fortawesome/fontawesome-free": "5.11.2",
    "@fortawesome/fontawesome-svg-core": "1.2.25",
    "@fortawesome/free-brands-svg-icons": "5.11.2",
    "@fortawesome/free-regular-svg-icons": "5.11.2",
    "@fortawesome/free-solid-svg-icons": "5.11.2",
    "ajv": "^6.10.2",
    "babel-loader": "8.0.6",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "2.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^3.1.0",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "sass-lint": "^1.13.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
